{?InputList}
	{#InputList}
		<h3>
			<small>#{Id}</small> {Name}
			{#Mandatory}
				<span class="label label-important">Mandatory</span>
			{:else}
				<span class="label label-info">Optional</span>
			{/Mandatory}
		</h3>
		<section>
			<p>
				<strong>Description</strong>
				{~n}
				{Description}
			</p>
		<section>
	{/InputList}
{:else}
	<em>No INPUTS defined</em>
{/InputList}


