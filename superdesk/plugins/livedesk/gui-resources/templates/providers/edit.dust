<div class="edit-block ">	<form>		<select class="" name="type">			<!--<option>_('Type of posts')</option>-->			{#PostTypes}				<option value="{Key}">{Key}</option>			{/PostTypes}		</select>		<textarea name="body-text"></textarea>		<button ci="savepost" class="btn btn-primary pull-right" type="submit">_('Save &amp; post')</button>		<button ci="save" class="btn pull-right" type="submit">_('Save')</button>	</form></div><div id="own-posts-holder" class="search-result-list-own scroller">    <ul id="own-posts-results" class="post-list wraped">    </ul></div>