<li class="google">
	<div class="result-content">
		<h3><a href="{url}">{title}</a></h3>
		<p class="result-text">{content}</p>
		<p class="attributes">
			<i class="source-icon">
				<img src="http://g.etfv.co/{url}" style="max-width: 16px" border="0">
			</i>
			<a class="author-name" href="{url}">{visibleUrl}</a>
			<time>Sat, Apr 21 2012 05:00:01</time>
		</p>
    </div>
</li>