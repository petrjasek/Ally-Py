{#results}
<li class="with-avatar twitter draggable">
	<div class="drag-bar"></div>
    <figure><img src="{profile_image_url}" width="48" height="48" alt="1"></figure>
    <div class="result-content">
        <h3><span class="author">{from_user_name}({from_user})</span></h3>
        <p class="result-text">{text|s}</p>
        <p class="attributes"><i class="source-icon"></i><time>{created_at}</time></p>
    </div>
</li>
{/results}